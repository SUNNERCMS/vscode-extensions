{
  "name": "easywork",
  "displayName": "easywork",
  "description": "简单开心工作",
  "version": "0.0.5",
  "publisher": "sunnercms",
  "engines": {
    "vscode": "^1.63.0"
  },
  "keywords": [
    "easywork",
    "translator",
    "snippests"
  ],
  "categories": [
    "Other"
  ],
  "icon": "images/iconImage.jpg",
  "activationEvents": [
    "onCommand:vscode-extension-demo.helloWorld",
    "onCommand:formatConversion.camelCase",
    "onCommand:formatConversion.constCase",
    "onCommand:formatConversion.snakeCase",
    "onCommand:formatConversion.paramCase"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "vscode-extension-demo",
      "properties": {
        "vscodePluginDemo.yourName": {
          "type": "string",
          "default": "默认: demo， 请输入your name",
          "description": "vscode插件开发示例-插件设置项"
        },
        "vscodePluginDemo.showTip": {
          "type": "boolean",
          "default": true,
          "description": "vscode插件开发示例: 是否在每次启动时显示欢迎提示！"
        }
      }
    },
    "keybindings": [
      {
        "command": "vscode-extension-demo.helloWorld",
        "key": "ctrl+y",
        "mac": "ctrl+y",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "vscode-extension-demo.helloWorld",
          "group": "navigation"
        },
        {
          "when": "editorFocus",
          "command": "formatConversion.camelCase",
          "group": "0_formatConversion@1"
        },
        {
          "when": "editorFocus",
          "command": "formatConversion.snakeCase",
          "group": "0_formatConversion@2"
        },
        {
          "when": "editorFocus",
          "command": "formatConversion.paramCase",
          "group": "0_formatConversion@3"
        },
        {
          "when": "editorFocus",
          "command": "formatConversion.constCase",
          "group": "0_formatConversion@4"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-extension-demo.helloWorld",
        "title": "Hello World Demo"
      },
      {
        "command": "formatConversion.camelCase",
        "title": "formatConversion"
      },
      {
        "command": "formatConversion.constCase",
        "title": "FORMAT_CONVERSION"
      },
      {
        "command": "formatConversion.snakeCase",
        "title": "format_conversion"
      },
      {
        "command": "formatConversion.paramCase",
        "title": "format-conversion"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/react.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2",
    "crypto": "^1.0.1",
    "md5-node": "^1.0.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SUNNERCMS/vscode-extension.git"
  },
  "bugs": {
    "url": "https://github.com/SUNNERCMS/vscode-extension/issues"
  },
  "homepage": "https://github.com/SUNNERCMS/vscode-extension#readme",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "author": "",
  "license": "ISC"
}
